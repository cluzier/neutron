{
  "name": "neutron",
  "productName": "Neutron",
  "version": "1.1.0",
  "description": "An application to locate app.asar files in Electron applications (Neutron)",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "dev": "next dev",
    "prod": "next build && electron ."
  },
  "keywords": [
    "electron",
    "asar",
    "neutron"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^19.1.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "minimatch": "^9.0.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react-dom": "^19.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.303.0",
    "next": "^14.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "build": {
    "appId": "com.neutron",
    "productName": "Neutron",
    "files": [
      "out/**/*",
      "main.js",
      "preload.js",
      "renderer.js",
      "package.json",
      "!**/node_modules/*/{test,__tests__,tests}/**",
      "!**/node_modules/.bin",
      "!**/*.map",
      "!**/src",
      "!**/test",
      "!**/tests",
      "!**/docs",
      "!**/release",
      "!**/.gitignore",
      "!**/.DS_Store"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "asar": true,
    "afterPack": "./scripts/remove-locales.js"
  }
}
